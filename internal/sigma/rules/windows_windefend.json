    {
      "id": "a0a278fe-2c0e-4de2-ac3c-c68b08a9ba98",
      "title": "LSASS Access Detected via Attack Surface Reduction",
      "status": "test",
      "description": "Detects Access to LSASS Process",
      "level": "high",
      "tags": [
        "attack.credential-access",
        "attack.t1003.001"
      ],
      "logsource": {
        "category": null,
        "product": "windows",
        "service": "windefend"
      },
      "detection": {
        "selections": {
          "selection": {
            "type": "field_match",
            "matchers": {
              "EventID": {
                "field": "EventID",
                "value": 1121,
                "modifiers": [],
                "is_list": false,
                "negated": false
              },
              "Path__endswith": {
                "field": "Path",
                "value": "\\lsass.exe",
                "modifiers": [
                  "endswith"
                ],
                "is_list": false,
                "negated": false
              }
            }
          }
        },
        "filters": {
          "filter_thor": {
            "type": "field_match",
            "matchers": {
              "ProcessName__startswith": {
                "field": "ProcessName",
                "value": "C:\\Windows\\Temp\\asgard2-agent\\",
                "modifiers": [
                  "startswith"
                ],
                "is_list": false,
                "negated": false
              },
              "ProcessName__endswith": {
                "field": "ProcessName",
                "value": [
                  "\\thor64.exe",
                  "\\thor.exe"
                ],
                "modifiers": [
                  "endswith"
                ],
                "is_list": true,
                "negated": false
              }
            }
          },
          "filter_exact": {
            "type": "field_match",
            "matchers": {
              "ProcessName": {
                "field": "ProcessName",
                "value": [
                  "C:\\Windows\\System32\\atiesrxx.exe",
                  "C:\\Windows\\System32\\CompatTelRunner.exe",
                  "C:\\Windows\\System32\\msiexec.exe",
                  "C:\\Windows\\System32\\nvwmi64.exe",
                  "C:\\Windows\\System32\\svchost.exe",
                  "C:\\Windows\\System32\\Taskmgr.exe",
                  "C:\\Windows\\System32\\wbem\\WmiPrvSE.exe",
                  "C:\\Windows\\SysWOW64\\msiexec.exe"
                ],
                "modifiers": [],
                "is_list": true,
                "negated": false
              }
            }
          },
          "filter_begins": {
            "type": "field_match",
            "matchers": {
              "ProcessName__startswith": {
                "field": "ProcessName",
                "value": [
                  "C:\\Windows\\System32\\DriverStore\\",
                  "C:\\WINDOWS\\Installer\\",
                  "C:\\Program Files\\",
                  "C:\\Program Files (x86)\\"
                ],
                "modifiers": [
                  "startswith"
                ],
                "is_list": true,
                "negated": false
              }
            }
          }
        },
        "condition": "selection and not 1 of filter_*",
        "condition_parsed": {
          "op": "and",
          "operands": [
            {
              "type": "ref",
              "value": "selection"
            },
            {
              "type": "not",
              "value": "1 of filter_*"
            }
          ]
        }
      },
      "mitre": {
        "techniques": [
          "T1003.001"
        ],
        "tactics": [
          "credential_access"
        ]
      },
      "meta": {
        "author": "Markus Neis",
        "date": "2018-08-26",
        "modified": "2022-08-13",
        "references": [
          "https://learn.microsoft.com/en-us/defender-endpoint/attack-surface-reduction"
        ],
        "falsepositives": [
          "Google Chrome GoogleUpdate.exe",
          "Some Taskmgr.exe related activity"
        ],
        "source_file": "sigma_rules/windows/builtin/windefend/win_defender_asr_lsass_access.yml"
      }
    },
