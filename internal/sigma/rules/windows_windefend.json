    {
      "id": "a3ab73f1-bd46-4319-8f06-4b20d0617886",
      "title": "Windows Defender Exploit Guard Tamper",
      "status": "test",
      "description": "Detects when someone is adding or removing applications or folders from exploit guard \"ProtectedFolders\" or \"AllowedApplications\"\n",
      "level": "high",
      "tags": [
        "attack.defense-evasion",
        "attack.t1562.001"
      ],
      "logsource": {
        "category": null,
        "product": "windows",
        "service": "windefend"
      },
      "detection": {
        "selections": {
          "allowed_apps_key": {
            "type": "field_match",
            "matchers": {
              "EventID": {
                "field": "EventID",
                "value": 5007,
                "modifiers": [],
                "is_list": false,
                "negated": false
              },
              "NewValue__contains": {
                "field": "NewValue",
                "value": "\\Windows Defender\\Windows Defender Exploit Guard\\Controlled Folder Access\\AllowedApplications\\",
                "modifiers": [
                  "contains"
                ],
                "is_list": false,
                "negated": false
              }
            }
          },
          "allowed_apps_path": {
            "type": "field_match",
            "matchers": {
              "NewValue__contains": {
                "field": "NewValue",
                "value": [
                  "\\Users\\Public\\",
                  "\\AppData\\Local\\Temp\\",
                  "\\Desktop\\",
                  "\\PerfLogs\\",
                  "\\Windows\\Temp\\"
                ],
                "modifiers": [
                  "contains"
                ],
                "is_list": true,
                "negated": false
              }
            }
          },
          "protected_folders": {
            "type": "field_match",
            "matchers": {
              "EventID": {
                "field": "EventID",
                "value": 5007,
                "modifiers": [],
                "is_list": false,
                "negated": false
              },
              "OldValue__contains": {
                "field": "OldValue",
                "value": "\\Windows Defender\\Windows Defender Exploit Guard\\Controlled Folder Access\\ProtectedFolders\\",
                "modifiers": [
                  "contains"
                ],
                "is_list": false,
                "negated": false
              }
            }
          }
        },
        "filters": {},
        "condition": "all of allowed_apps* or protected_folders",
        "condition_parsed": {
          "op": "or",
          "operands": [
            "all of allowed_apps*",
            "protected_folders"
          ]
        }
      },
      "mitre": {
        "techniques": [
          "T1562.001"
        ],
        "tactics": [
          "defense_evasion"
        ]
      },
      "meta": {
        "author": "Nasreddine Bencherchali (Nextron Systems)",
        "date": "2022-08-05",
        "modified": "2022-12-06",
        "references": [
          "https://techcommunity.microsoft.com/t5/core-infrastructure-and-security/windows-10-controlled-folder-access-event-search/ba-p/2326088"
        ],
        "falsepositives": [
          "Unlikely"
        ],
        "source_file": "sigma_rules/windows/builtin/windefend/win_defender_config_change_exploit_guard_tamper.yml"
      }
    },
