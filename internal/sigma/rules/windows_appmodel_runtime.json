      "title": "Sysinternals Tools AppX Versions Execution",
      "status": "test",
      "description": "Detects execution of Sysinternals tools via an AppX package.\nAttackers could install the Sysinternals Suite to get access to tools such as psexec and procdump to avoid detection based on System paths.\n",
      "level": "low",
      "tags": [
        "attack.defense-evasion",
        "attack.execution"
      ],
      "logsource": {
        "category": null,
        "product": "windows",
        "service": "appmodel-runtime"
      },
      "detection": {
        "selections": {
          "selection": {
            "type": "field_match",
            "matchers": {
              "EventID": {
                "field": "EventID",
                "value": 201,
                "modifiers": [],
                "is_list": false,
                "negated": false
              },
              "ImageName": {
                "field": "ImageName",
                "value": [
                  "procdump.exe",
                  "psloglist.exe",
                  "psexec.exe",
                  "livekd.exe",
                  "ADExplorer.exe"
                ],
                "modifiers": [],
                "is_list": true,
                "negated": false
              }
            }
          }
        },
        "filters": {},
